{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PublisherList",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer"
      },
      "name": {
        "type": "string"
      },
      "address": {
        "type": "string"
      },
      "contactInfo": {
        "type": "string"
      }
    },
    "required": ["id", "name", "address", "contactInfo"],
    "additionalProperties": false
  }
}