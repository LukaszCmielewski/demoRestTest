{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MemberList",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer"
      },
      "firstName": {
        "type": "string"
      },
      "lastName": {
        "type": "string"
      },
      "address": {
        "type": "string"
      },
      "phone": {
        "type": "string"
      },
      "email": {
        "type": "string",
        "format": "email"
      },
      "registrationDate": {
        "type": "string",
        "format": "date"
      }
    },
    "required": [
      "id", "firstName", "lastName", "address", "phone", "email", "registrationDate"
    ],
    "additionalProperties": false
  }
}
